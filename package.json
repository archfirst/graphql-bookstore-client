{
  "name": "graphql-bookstore-client",
  "description": "Example demonstrating the patterns in real-world GraphQL applications and implementing them using Apollo GraphQL",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/archfirst/graphql-bookstore-client.git"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "inquirer-directory": "^2.1.0",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "plop": "^1.8.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.0",
    "react-dev-utils": "^4.0.1",
    "react-error-overlay": "^1.0.10",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.10",
    "material-ui-icons": "^1.0.0-beta.10",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "subscriptions-transport-ws": "^0.7.3",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint 'src/**/*.js'",
    "gen": "plop --plopfile generators/plopfile.js",
    "format": "prettier --print-width 80 --tab-width 4 --single-quote --write 'src/**/*.js'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 80 --tab-width 4 --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  }
}
